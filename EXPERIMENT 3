//EXPERIMENT 3-A
#include<iostream>
using namespace std;
int main()
{
    int a, b, c, d, sum;
    cout<<"Enter the marks of sub 1: ";
    cin>>a;
    cout<<"Enter the marks of sub 2: ";
    cin>>b;
    cout<<"Enter the marks of sub 3: ";
    cin>>c;
    cout<<"Enter the marks of sub 4: ";
    cin>>d;
    sum = (a+b+c+d)/100;
    if(90>=sum){
        cout<<"O grade";

    }
    else if(90>sum>=85){
        cout<<"A+ grade";
    }
    else if(85>sum>=75){
        cout<<"A grade";

    }
    else if(75>sum>=55){
        cout<<"B grade";
    }
    else if(55>sum>=40){
        cout<<"C grade";
    }
    else{
        cout<<"F grade";
    }   
}
 //OUTPUT
Enter the marks of sub 1: 85
Enter the marks of sub 2: 84
Enter the marks of sub 3: 95
Enter the marks of sub 4: 87
O grade

//EXPERIMENT 3-B
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int b;
    cout<<"Enter a PRN: ";
    cin>>b;
    int c;
    while (b>6)
    {
        c= b%10;
        b/=10;
        cout<<c<<endl;
        

        
    }
    cout<<b;
}

//OUTPUT
Enter a PRN: 123110
0
1
1
3
2
1
